{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Qwik App",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": ["es2022", "DOM", "WebWorker", "DOM.Iterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik",
    "allowJs": true,
    "noEmit": true
  }
}
